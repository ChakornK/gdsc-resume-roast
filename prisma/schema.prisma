generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resume {
  id      String   @id @default(cuid())
  link    String
  reviews Review[]
}

model Review {
  id         String @id @default(cuid())
  formatting Int
  relevance  Int
  structure  Int
  clarity    Int
  wording    Int
  comments   String
  resumeId   String
  resume     Resume @relation(fields: [resumeId], references: [id])
}
